@{
  Layout="Shared/_Layout.cshtml";
}

<div class="container">
  <nav class="navbar navbar-inverse">
    <div class="container-fluid">
      <ul class="nav navbar-nav">
        <li><a href="/">Home</a></li>
        <li><a href="/tracks">Tracks</a></li>
        <li><a href="/instructors">Instructors</a></li>
        <li><a href="/schools">Schools</a></li>
        <li><a href="/account/create">Create Account</a></li>
        <li><a href="/account/login">Login</a></li>
      </ul>
    </div>
  </nav>
  <h3>Students Enrolled in @Model["selectedTrack"].GetName()</h3>
</div>
@foreach(var student in Model["allStudents"])
{
  <li><a href="/student/details/@student.GetId()">@student.GetLastName(), @student.GetFirstName()</a></li>
}
