@{
  Layout="Shared/_Layout.cshtml";
}
<div class="container text-center">
    <nav class="navbar navbar-inverse">
      <div class="container-fluid">
        <ul class="nav navbar-nav">
          <li><a href="/">Home</a></li>
          <li><a href="/tracks">Tracks</a></li>
          <li><a href="/instructors">Instructors</a></li>
          <li><a href="/schools">Schools</a></li>
          @if (@Model != null && !string.IsNullOrEmpty(Model.GetUserName()))
          {
            <li><a href="/student/details/@Model.GetId()">@Model.GetUserName()</a></li>
          }
          else
          {
            <li><a href="/account/create">Create Account</a></li>
            <li><a href="/account/login">Login</a></li>
          }

        </ul>
      </div>
  </nav>
  <div class="row">
    <div class="col-md-6">
      <img src="/Content/img/logo.jpg">
    </div>
    <div class="col-md-6">
      <h4>Welcome to KickStart University</h4>
      <h5>Where Dreams get......Kick started...</h5>
    </div>
  </div>
</div>
